{
  "name": "aurora-artifact-deployer",
  "groupId": "no.skatteetaten.aurora",
  "description": "Library for uploading artifacts",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "registry": "https://nexus-npm.aurora.skead.no/npm/repository/npm-internal/"
  },
  "scripts": {
    "prebuild": "npm-run-all -l clean lint",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "npm test -- --coverage",
    "lint": "eslint --ext .ts ./src",
    "lint:fix": "eslint --fix --ext .ts ./src",
    "clean": "rimraf lib coverage *.tgz"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-typescript": "7.3.3",
    "@types/jest": "24.0.18",
    "@types/node": "10.14.1",
    "@typescript-eslint/eslint-plugin": "2.0.0",
    "@typescript-eslint/parser": "2.0.0",
    "aurora-ci-scripts": "0.4.0",
    "eslint": "6.2.2",
    "eslint-config-prettier": "6.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "jest": "24.9.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "typescript": "3.6.2"
  },
  "prettier": {
    "singleQuote": true
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript"
    ]
  }
}
